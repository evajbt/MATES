<div class="container">
  <div class="row col-4">
    <% if @search_results.empty? %>
      <p>No mates found for you</p>
    <% else %>
    <% @search_results.each do |search_result| %>
      <%= search_result.username %>
      <%= search_result.birthdate %>
      <%= search_result.gender %>
    <br>
    <% search_result.user_games.each do |user_game| %>
      <%= user_game.mood %>
      <%= user_game.level %>
      <%= user_game.game.name %>
    <% end %>
  <% end %>
<% end %>
</div>
</div>
