<%= form_with(model: @profile, url: profile_path, method: :patch) do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %>

  <%= form.label :age %>
  <%= form.number_field :age %>

  <%= form.label :gender %>
  <%= form.select :gender, ['Male', 'Female', 'Other'] %>

  <%= form.label :email %>
  <%= form.email_field :email %>

  <%= form.label :password %>
  <%= form.text_field :password %>

  <%= form.submit 'Update' %>
<% end %>
